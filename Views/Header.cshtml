@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Header>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = null;
}

<header class="header-homepage bg-z-dark text-white d-flex">
    <div class="container my-auto">
        <div class="row">
            <div class="col-md-10 mx-auto">
                
                <h1 class="header-title">
                    @Model.Content.Title
                </h1>
                
                <h3 class="header-subtitle"> 
                    @Model.Content.Subtitle 
                </h3>
                
            </div>
        </div>
    </div>
</header>