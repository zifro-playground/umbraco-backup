@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.ZifroHome>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = "Master.cshtml";
}

@{
    var childNodes = Model.Content.Children.Where(x => x.DocumentTypeAlias != "Articulate");
}

@foreach(var child in childNodes)
{
    @Umbraco.RenderTemplate(child.Id, child.TemplateId)
}